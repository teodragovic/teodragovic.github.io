{
  "name": "teodragovic",
  "version": "2.0.0",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch styles:_includes/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass --no-source-map --style=compressed styles:_includes/css",
    "build:eleventy": "eleventy",
    "clean": "del-cli _site _includes/css",
    "optimize:sass": "postcss _includes/css/*.css -u autoprefixer cssnano -r",
    "start": "npm-run-all clean build:sass --parallel watch:*",
    "build": "npm-run-all clean build:sass optimize:sass build:eleventy",
    "github": "gh-pages -b master -d build",
    "commit:build": "git add . && git commit -m \"Build\" && git push origin source",
    "deploy": "npm run build && npm run github && npm run commit:build",
    "eslint": "eslint --cache --ext .js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "del-cli": "^3.0.1",
    "eslint": "^7.22.0",
    "gh-pages": "^3.1.0",
    "luxon": "^1.25.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.8"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
