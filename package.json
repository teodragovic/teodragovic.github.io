{
  "name": "teodragovic",
  "version": "2.0.0",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch styles:_site/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass --no-source-map styles:_site/css",
    "build:eleventy": "eleventy",
    "clean": "del-cli _site",
    "postbuild": "postcss _site/css/*.css -u autoprefixer cssnano -r",
    "start": "npm-run-all clean build:sass --parallel watch:*",
    "build": "npm-run-all clean build:sass build:eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "del-cli": "^3.0.1",
    "eslint": "^7.22.0",
    "luxon": "^1.25.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.8"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
